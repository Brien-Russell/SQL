--Updated complete database of food either created or found in Valheim
CREATE TABLE valheimFood (id INTEGER PRIMARY KEY AUTOINCREMENT, name TEXT, ingredients TEXT, health INTEGER, stamina INTEGER, healing INTEGER, duration INTEGER);

